            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Fecha Nacimiento</strong> </label>
                <input type="date" style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="FechaNacimiento" name="FechaNacimiento" />
            </div>

            <div class="col-xs-4 col-sm-4 col-md-4">
                <label><strong>Edad</strong></label>
                <input style="border: black; border-width: 1px; border-style: solid;" class="form-control" id="Edad" name="Edad" placeholder="Digite su edad" disabled />
            </div>
            <script>
                // Obtener los elementos del DOM
                const fechaNacimientoInput = document.getElementById('FechaNacimiento');
                const edadPoliticaInput = document.getElementById('Edad');

                // Agregar un evento de escucha al cambio en el campo de fecha de nacimiento
                fechaNacimientoInput.addEventListener('change', function () {
                    // Obtener la fecha seleccionada
                    const fechaNacimiento = new Date(this.value);

                    // Obtener la fecha actual
                    const fechaActual = new Date();

                    // Calcular la edad en años
                    let edad = fechaActual.getFullYear() - fechaNacimiento.getFullYear();

                    // Verificar si el cumpleaños aún no ha pasado este año
                    if (fechaNacimiento.getMonth() > fechaActual.getMonth() || (fechaNacimiento.getMonth() === fechaActual.getMonth() && fechaNacimiento.getDate() > fechaActual.getDate())) {
                        edad--;
                    }

                    // Actualizar el valor del campo de edad
                    edadPoliticaInput.value = edad;
                });
            </script>
